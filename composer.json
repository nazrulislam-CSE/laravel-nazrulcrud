{
    "name": "nazrulcrud/crud",
    "description": "A Powerful Laravel CRUD Generator with Bootstrap UI",
    "keywords": [
        "laravel",
        "crud",
        "generator",
        "nazrul",
        "bootstrap"
    ],
    "type": "library",
    "license": "MIT",

    "autoload": {
        "psr-4": {
            "NazrulCrud\\Crud\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "NazrulCrud\\Crud\\Tests\\": "tests/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "NazrulCrud\\Crud\\CrudServiceProvider"
            ]
        }
    },

    "require": {
        "php": "^8.1",
        "laravel/framework": "^12.0"
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
